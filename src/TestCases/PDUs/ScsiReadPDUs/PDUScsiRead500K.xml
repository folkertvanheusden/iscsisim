<!-- Command:        0x01 - SCSI Command                -->
<!-- Tfer Size:      500K                                -->
<!-- Immediate data: No                                 -->
<!-- Function:       Read 500 KB from LUN 0                -->
  
<!-- All values are HEX -->

<ISCSIPDU>
    <BHS>                               <!-- basic header segment start      -->
        <DWORD1>41400000</DWORD1>       <!-- operation code                  -->
        <AHSLEN>00</AHSLEN>             <!-- additional header seg. length   -->
