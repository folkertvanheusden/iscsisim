<!-- Filename:       PDUSecLoginContinuersp.xml                 -->
<!-- Command:        0x23 - Login Response               -->
<!-- Tfer Size:      BHS_SIZE                                -->
<!-- Immediate data: No                                 -->
<!-- Function:       Response to Login Request with Continue bit set, in security stage                -->
<ISCSIPDU>
	<BHS>
		<DWORD1>23000000</DWORD1> <!-- T=0, C=0, CSG = security, NSG is reserved  -->
		<AHSLEN>00</AHSLEN>
		<DATASEGLEN>00</DATASEGLEN>
		<LUN>4000013700000000</LUN>	   <!-- this is actually isid/tsih -->
		<ITT>$ITT</ITT>
		<DW20>0</DW20>
		<DW24>$X</DW24>
		<DW28>$X</DW28>
		<DW32>$X</DW32>
		<DW36>0</DW36>
		<DW40>0</DW40>
		<DW44>0</DW44>
	</BHS>
	<!-- AHS is not represented -->
	<HEADERDIGEST />
	<DATA/>
	<DATADIGEST />
</ISCSIPDU>
