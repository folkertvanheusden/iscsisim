		<DW20>00010000</DW20>
		<DW24>$CMD_SN</DW24>
		<DW28>$EXP_STAT_SN</DW28>
		<DW32>0</DW32>
		<DW36>0</DW36>
		<DW40>0</DW40>
		<DW44>0</DW44>
	</BHS>
